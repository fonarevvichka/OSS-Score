{
    "name": "OSS Score",
    "version": "0.0.1",
    "description": "Adds Repo Activity and License Scores",
    "content_scripts": [ {
      "js": [ "extension.js"],
      "matches": [ "https://github.com/*" ],
      "css": ["extension.css"]
    }],
    "web_accessible_resources": [{
      "resources": ["contentscript.css", "images/neverGiveUp.png"],
      "matches": ["<all_urls>"]
    }],
    "manifest_version": 3
  }