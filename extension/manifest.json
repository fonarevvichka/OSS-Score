{
    "name": "OSS Score",
    "version": "0.0.1",
    "description": "Adds Repo Activity and License Scores",
    "action": {
      "default_icon": {                
        "16": "images/icon16.png",    
        "24": "images/icon24.png",   
        "32": "images/icon32.png"  
      },
      "default_title": "OSS Score"  
    },
    "content_scripts": [ {
      "js": [ "extension.js"],
      "matches": [ "https://github.com/*" ],
      "css": ["extension.css"]
    }],
    "web_accessible_resources": [{
      "resources": ["contentscript.css", "images/neverGiveUp.png"],
      "matches": ["<all_urls>"]
    }],
    "manifest_version": 3
  }